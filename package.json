{
  "name": "NavigoOMNIAPIs",
  "version": "0.0.1",
  "description": "NavigoOMNIAPIs",
  "type": "commonjs",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsoa spec-and-routes && tsc && tsc-alias",
    "tsoa": "tsoa spec-and-routes",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migrate": "typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "typeorm:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source.ts",
    "typeorm:generate": "typeorm-ts-node-commonjs migration:generate -d src/data-source.ts",
    "typeorm:create": "typeorm-ts-node-commonjs migration:create",
    "monitor": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\" \"node src/receive.ts\" \"npm run lint:fix\" \"node src/sio-server.ts\"",
    "watch": "onchange 'src/**/*.ts' -- npm run monitor",
    "monitor:debug": "concurrently \"nodemon --inspect\" \"nodemon -x tsoa spec-and-routes\""
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/geojson": "^7946.0.14",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^16.11.10",
    "@types/node-cron": "^3.0.11",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5",
    "typescript-eslint": "^7.18.0"
  },
  "dependencies": {
    "@types/pg": "^8.11.10",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "crc-32": "^1.2.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "geojson": "^0.5.0",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "net": "^1.0.2",
    "node-cron": "^3.0.3",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.6.0",
    "typeorm": "^0.3.20",
    "websocket": "^1.0.35",
    "wkx": "^0.5.0",
    "ws": "^8.18.0"
  }
}
